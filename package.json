{
  "name": "majal",
  "version": "0.0.0",
  "scripts": {
    "proxy": "tarima -wdV -P localhost:8001 -- node app.js",
    "watch": "tarima -wsdV",
    "build": "tarima -f",
    "start": "npm run build && node app.js"
  },
  "tarima": {
    "src": "src",
    "dest": "build",
    "cache": "build/index.json",
    "filter": [
      "!_*/**",
      "!**/_*/**",
      "!**/_*"
    ],
    "ignoreFiles": [
      ".gitignore"
    ],
    "serverOptions": {
      "routes": {
        "/": "build/public",
        "/vendor": "bower_components"
      }
    }
  },
  "dependencies": {
    "express": "^4.13.4",
    "ractive": "^0.7.3"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "kramed": "^0.5.6",
    "less": "^2.6.1",
    "pug": "^2.0.0-alpha1",
    "tarima-cli": "git+https://github.com/gextech/tarima-cli.git#next"
  }
}
